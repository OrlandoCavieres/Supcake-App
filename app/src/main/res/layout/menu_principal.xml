<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ghost"
    android:paddingLeft="0dp"
    android:paddingTop="0dp"
    android:paddingRight="0dp"
    android:paddingBottom="0dp"
    tools:context=".MenuPrincipal">

    <include
        android:id="@+id/include2"
        layout="@layout/borde_menu_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/img_dulces1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="272dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintEnd_toStartOf="@+id/img_torta1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cake_image2" />

    <ImageView
        android:id="@+id/img_torta1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="272dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintEnd_toStartOf="@+id/img_dulces2"
        app:layout_constraintStart_toEndOf="@+id/img_dulces1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cake_image1" />

    <ImageView
        android:id="@+id/img_dulces2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="272dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintEnd_toStartOf="@+id/img_torta2"
        app:layout_constraintStart_toEndOf="@+id/img_torta1"
        app:layout_constraintTop_toTopOf="@+id/include2"
        app:srcCompat="@drawable/cake_image2" />

    <ImageView
        android:id="@+id/img_torta2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="272dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintEnd_toStartOf="@+id/img_dulces3"
        app:layout_constraintStart_toEndOf="@+id/img_dulces2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cake_image1" />

    <ImageView
        android:id="@+id/img_dulces3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="272dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_torta2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cake_image2" />

    <ImageView
        android:id="@+id/img_dulces4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="104dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintBottom_toBottomOf="@+id/include2"
        app:layout_constraintEnd_toStartOf="@+id/img_torta3"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cake_image2" />

    <ImageView
        android:id="@+id/img_torta3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="104dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintBottom_toBottomOf="@+id/include2"
        app:layout_constraintEnd_toStartOf="@+id/img_dulces5"
        app:layout_constraintStart_toEndOf="@+id/img_dulces4"
        app:srcCompat="@drawable/cake_image1" />

    <ImageView
        android:id="@+id/img_dulces5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="104dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintBottom_toBottomOf="@+id/include2"
        app:layout_constraintEnd_toStartOf="@+id/img_torta4"
        app:layout_constraintStart_toEndOf="@+id/img_torta3"
        app:srcCompat="@drawable/cake_image2" />

    <ImageView
        android:id="@+id/img_torta4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="104dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/img_dulces6"
        app:layout_constraintStart_toEndOf="@+id/img_dulces5"
        app:srcCompat="@drawable/cake_image1" />

    <ImageView
        android:id="@+id/img_dulces6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="104dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/img_torta4"
        app:srcCompat="@drawable/cake_image2" />

    <Button
        android:id="@+id/boton_nuevaVenta"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="206dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/menu_rectangle"
        android:text="@string/txt_nuevaVenta"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:textColor="@color/white"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_dulces2" />

    <Button
        android:id="@+id/boton_clientes"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="206dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/menu_rectangle"
        android:fontFamily="@font/fira_mono_medium"
        android:text="@string/txt_clientes"
        android:textColor="@color/white"
        android:textSize="26sp"
        android:verticalScrollbarPosition="left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_nuevaVenta" />

    <Button
        android:id="@+id/boton_catalogo"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="206dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/menu_rectangle"
        android:fontFamily="@font/fira_mono_medium"
        android:text="@string/txt_catalogo"
        android:textColor="@color/white"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_clientes" />

    <ImageView
        android:id="@+id/icon_properties_menu"
        android:layout_width="59dp"
        android:layout_height="59dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="44dp"
        android:contentDescription="@string/sinTexto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/properties" />

    <TextView
        android:id="@+id/txt_modAdministrador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginLeft="152dp"
        android:layout_marginBottom="48dp"
        android:contentDescription="@string/sinTexto"
        android:fontFamily="@font/fira_mono_medium"
        android:gravity="end"
        android:text="@string/txt_modoAdministrador"
        android:textColor="@color/zucinni"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/icon_properties_menu"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/boton_estadisticas"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="206dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/menu_rectangle"
        android:fontFamily="@font/fira_mono_medium"
        android:text="@string/txt_estadisticas"
        android:textColor="@color/white"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_catalogo" />

</androidx.constraintlayout.widget.ConstraintLayout>